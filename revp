from Bio import Seq

data = '''
TATAGTACATTTGTGCGAAGCCTTAATAAAGATGCAGCCGAGTTTGTATGCCCCGACAGT
GAACTTCACCTGGTATCTCGTCTAGGGTTACCACACTGTGATTGTTTCTATAACTCTAAA
AGTCGCTCGCGGCTAATAAATATCTGATGGCTAACCTCGTTTGAGTCTCATGGTCTTGGT
GTCTATTACCGAACAGCGGCCTTAGACAGAAGAATCCCCATTGATCGACACTCGTTAGAC
GTCCACTTCTACGTACCGGGAGAACATTTTCGAAAATGTCAGAGCGAACTCACCACCAAT
CATTCAATTCTGGGAACCTTCATGTTAGAACGGTAAACGATGGAGGGTTATCTGTCACGC
TAACGGCGCAAACTATATGAACACGCCTTATTCTCGGCAAGCGGATTAGAAAAACTTACT
ACCCTCTGTTAGGCTGATCGTAGGCGCCTACCGGGGACACAGCCAACACAATAGGTCTTT
AATACAAGAGACATCCACGCGCAGCTAGGTGGTACTCTGAGCGGAATTTAGACTTGCGTC
ACCCTAATCGATATTAATATGGAGTCCAGCAGAGACATTACCTGCATCAGACCATGTAAG
ATGCATAGGCCCAGGCCGTCTTTTGCCGTCGACTCCTACAATGGCCATTAAGCAGTTCCC
ATCTCAGGATGGGCACTGAACTACGCGGTCGTGCACTTGCTTGATTGGGGGGGAGTACCT
TGCTGCTATCCTGATCTTCTCCTTTCTACGCTAGATGGCTGGTCACCGAGAGCCCATGCG
TATCCGGAGGCACCTAGCAGACTCGTCCCTACTTAGATTTCTAATGTCCACCTTAGGTTG
ATGTTGCACGTACAATATGACAATGCACCGGCCATGGAGATCTAAACGCG

'''
data = "".join(data.split())
print len(data)
outfile = open('outfile.txt', 'w')
answer = list()

for col in range(0,len(data)):
	for i in range(col+1, len(data)):
		if data[col:i+1] == Seq.reverse_complement(data[col:i+1]) and len(data[col:i+1]) > 3:
			answer.append(str(col+1) + ' ' + str(len(data[col:i+1])))
				
for item in answer:
	outfile.write(item+'\n')
	print (item)
